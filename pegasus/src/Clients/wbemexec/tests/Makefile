	@$(ECHO) "wbemexec -xfoo < ./cimrequest.xml" >> $(RESULTFILE)
	@$(ECHO) " " >> $(RESULTFILE)
	@wbemexec -xfoo < ./cimrequest.xml >> $(RESULTFILE) $(REDIRECTERROR)
