	@$(ECHO) " " >> $(RESULTFILE)
	@wbemexec -h < ./cimrequest.xml >> $(RESULTFILE) $(REDIRECTERROR)
	@$(ECHO) " " >> $(RESULTFILE)
	@$(ECHO) " " >> $(RESULTFILE)
	@$(ECHO) "Missing argument for -p option" >> $(RESULTFILE)
	@$(ECHO) "wbemexec -p < ./cimrequest.xml" >> $(RESULTFILE)
