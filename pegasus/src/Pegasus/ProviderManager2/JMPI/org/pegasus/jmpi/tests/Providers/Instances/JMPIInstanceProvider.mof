// ==================================================================
// JMPIInstance_TestElement
// ==================================================================
[ Abstract,
  Version ("2.2.0"),
  Description ("JMPIInstance_TestElement is an abstract class that provides a common "
               "superclass (or top of the inheritance tree) for the "
               "non-association classes in the test Schema.")
]
class JMPIInstance_TestElement
{
   [ Key,
     MaxLen (256),
     Description ("CreationClassName indicates the name of the class or the "
                  "subclass used in the creation of an instance. When used "
                  "with the other key properties of this class, this property "
                  "allows all instances of this class and its subclasses to "
                  "be uniquely identified.")
   ]
   string CreationClassName;

   [ Key,
     Description ("An identifier that uniquely identifies each instance "
                  "of the JMPIInstance_TestElement hierarchy.")
   ]
   uint64 InstanceId;
};

